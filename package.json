{
  "version": "2.27.0",
  "name": "cn-shell",
  "description": "Cloud Native Shell",
  "main": "./dist/cn-shell.js",
  "bin": "",
  "files": [
    "dist/**/*"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ],
    "**/*.{json,md,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "trailingComma": "all"
  },
  "scripts": {
    "build": "tsc",
    "format-ts": "prettier --write 'src/**/*.ts'",
    "format-js": "prettier --write 'dist/**/*.{js,ts}'",
    "format-misc": "prettier --write '**/*.{json,md,yaml}'",
    "install-bin": "npm install -g $(npm pack . | tail -1)",
    "test": "echo \"No tests\" && exit 0",
    "clean": "rm -rf ./dist",
    "prepare": "npm run clean && npm run format-ts && npm run format-misc && tsc",
    "prepublishOnly": "npm run format-js && npm test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "Cloud",
    "Native",
    "Application",
    "Framework",
    "CNA"
  ],
  "author": "Black Eye Technology",
  "license": "MIT",
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^1.19.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@koa/multer": "^2.0.2",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-helmet": "^6.0.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.1.1",
    "@types/koa__multer": "^2.0.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "^12.20.47",
    "axios": "^0.25.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^4.0.1",
    "koa-helmet": "^4.2.1",
    "koa-router": "^7.4.0",
    "minimist": "^1.2.6",
    "multer": "^1.4.4",
    "openapi-validator-middleware": "^3.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackeyetech/cn-shell.git"
  },
  "bugs": {
    "url": "https://github.com/blackeyetech/cn-shell/issues"
  },
  "homepage": "https://github.com/blackeyetech/cn-shell#readme"
}
